    <!-- prevent theme flash -->
    <style>
      html.dark {
        background: #171B25;
      }
      html.light {
        background: #fff;
        opacity: 0;
        transition: opacity 0.2s ease;
      }
    </style>
    <noscript>
      <style>
        html.light {
          opacity: 1;
        }
      </style>
    </noscript>
    <script>
      var htmlEl = document.querySelector("html");
      const darkThemes = new Set(["dark", "night"])
      const siteTheme = document.querySelector("#site_theme").dataset.siteTheme
      const osPrefersLightTheme = window.matchMedia(
        "(prefers-color-scheme: light)"
      ).matches;
      if (osPrefersLightTheme) {
        htmlEl.classList.add("light");
      } else if (!osPrefersLightTheme) {
        htmlEl.classList.add("dark");
      }
      // override any prefers-color-scheme logic with site_theme here
      if (siteTheme && darkThemes.has(siteTheme)) {
        htmlEl.classList.remove("light")
        htmlEl.classList.remove("dark")
        htmlEl.classList.add("dark")
      }
      var isSafari = window.navigator.userAgent.toLowerCase().match('safari')
      var TIMEOUT = isSafari ? 300 : 50;
      if (osPrefersLightTheme) {
        window.addEventListener('load', function() {
          var el = document.querySelector("html.light");
          if (el) {
            setTimeout(function(){
              el.style.opacity = "1";
            }, TIMEOUT)
          }
        })
      }
    </script>
    <!-- end prevent theme flash -->

