/* Manual tests */
  (function (w, d) {
    var ad_layout_test = Math.random() < 0.5;
    w.adthrive = w.adthrive || {};
    w.adthrive.cmd = w.adthrive.cmd || [];
    if (ad_layout_test) {
      d.getElementsByTagName('head')[0].className += ' adthrive-test-16';
    }
w.adthrive.cmd.push(function () {
       w.adthrive.config.abGroup.set('pubtst16', ad_layout_test ? 'on' : 'off');
    });
  })(window, document);
